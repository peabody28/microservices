{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/${input:projectName}/${input:projectName}.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": []
        },
    ],
    "inputs": [
        {
          "type": "pickString",
          "id": "projectName",
          "description": "What project yoy want to start?",
          "options": [
            "auth",
            "wallet",
            "risk.score",
            "payment",
            "user"
          ],
          "default": "user"
        },
    ]
}